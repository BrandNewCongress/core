<script>
  window.showHiddenContent = function (ev) {
    var el = document.querySelector('#vol-event');
    if (el.style.display == 'hidden') {
      el.style.display = 'block';
    } else {
      el.style.display = 'none';
    }
  }
</script>
<div style="padding: 40px;">
  <%= raw(@content) %>
</div>
